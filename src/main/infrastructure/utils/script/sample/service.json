{
	"id": "access-control-service-ldap",
	"container": {
		"portMappings": [
			{
				"containerPort": 80,
				"hostPort": 0,
				"servicePort": 10000
			}
		],
		"type": "MESOS",
		"volumes": [],
		"docker": {
			"image": "nginx",
			"forcePullImage": false,
			"pullConfig": {
				"secret": "pullConfigSecret"
			}
		}
	},
	"env": {
		"A": "1",
		"B": "2",
		"C": "3"
	},
	"secrets": {
		"pullConfigSecret": {
			"source": "/mesos-docker/pullConfig"
		}
	},
	"args": [],
	"cpus": 1,
	"disk": 0,
	"instances": 1,
	"mem": 256,
	"networks": [
		{
			"mode": "container/bridge"
		}
	],
	"requirePorts": false
}
